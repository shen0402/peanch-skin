/*modal start*/
.value-props-modal,
.modal-box {
    z-index: 9999;
}

.modal-sandbox {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: transparent;
}

.value-props-modal {
    display: none;
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: rgba(0, 0, 0, .4);
    overflow: auto;
    margin: 0;
}

.modal-box {
    position: absolute;
    width: 98%;
    max-height: 100%;
    max-width: 780px;
    margin: 0px auto;
    left: 0;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
}

.modal-header {
    padding: 20px 40px;
    background: #546E7A;
    color: #ffffff;
}

.modal-body {
    background: #fff;
