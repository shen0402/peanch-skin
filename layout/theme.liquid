<script>
const closeButtonInterval = [800, 1000, 1200, 2000, 2500, 5000, 9000] 
// Flag
let addButton = true
  
async function superBarCloseButton() {
  const superbarLoaded  = await window.SUPERBAR
  //html X button
  const closeButton =  document.createElement('span')
  closeButton.setAttribute('class','close-button')
  //styling with Safari
  closeButton.setAttribute('style','position: relative; display: flex; justify-content: end; cursor: pointer; padding-right: 10px;')
  closeButton.innerHTML = 'X'
  
  //hide supperBar if the close button is clicked
  if (superbarLoaded && addButton){
    try{
    document.querySelector('#sabhq-root form').prepend(closeButton)
    closeButton.addEventListener('click',function(){
      document.querySelector('#sabhq-root').style.display = 'none' 
    })
    addButton = false 
  } catch(err){}
  } 
}
closeButtonInterval.forEach(i => setTimeout(superBarCloseButton, i) )  
</script>
  
